				},
				{
				 	"LogIndexer":				
								 {
								 	"formatDate":"yyyy MMM dd",
								 	"customFilter":[{"%T":"patter2.1"},{"%g":"patter2.4"},{"%L":"patter2.2"},{"%l":"patter2.3"}],
								 	"fileName":"indexerLog",
